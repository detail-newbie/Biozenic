<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    
    <t t-inherit="web.JsonField" t-inherit-mode="extension">
        <xpath expr="//span" position="before">
            <pre 
                t-if="props.prettify"
                class="mk_json_pretty" 
                t-esc="formattedValue"
            />
        </xpath>
        <xpath expr="//span" position="attributes">
            <attribute name="t-else">""</attribute>
        </xpath>
    </t>

</templates>
