<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="view_res_config_settings_form" model="ir.ui.view">
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="base_setup.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//block[@name='contacts_setting_container']" position="inside">
                <setting help="Activate to automatically generate contact numbers">
                    <field name="active_contact_number_automation" string="Contact Number Automation"/>
                </setting>
            </xpath>
        </field>
    </record>

</odoo>
